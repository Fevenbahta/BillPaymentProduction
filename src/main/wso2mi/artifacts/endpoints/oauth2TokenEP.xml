<?xml version="1.0" encoding="UTF-8"?>
<endpoint name="oauth2TokenEP" xmlns="http://ws.apache.org/ns/synapse">
    <http method="post" uri-template="https://developer.anbesabank.et/oauth2/token">
        <suspendOnFailure>
            <initialDuration>-1</initialDuration>
            <progressionFactor>1</progressionFactor>
        </suspendOnFailure>
        <markForSuspension>
            <retriesBeforeSuspension>0</retriesBeforeSuspension>
        </markForSuspension>
    </http>
</endpoint>
